        "exact": "sleep"
    },
    {
        "name": "Powershell",
        "desc": "Öffnet die Powershell",
        "icon": "$imgPath/windows/powershell.png",
        "type": "command",
        "command": "cd  %userprofile% & start cmd /c powershell",
        "exact": "ps"
    },
    {
        "name": "Kommandozeile",
        "desc": "Öffnet die Kommandozeile",
        "icon": "$imgPath/windows/cmd.png",
